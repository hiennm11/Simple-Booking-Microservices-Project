{
  "format": "seq-dashboard-export-2025.2",
  "version": "1.0",
  "description": "Seq 2025.2 native dashboard format for Simple Booking Microservices retry monitoring",
  "dashboards": [
    {
      "OwnerId": null,
      "Title": "Retry Overview Dashboard",
      "IsProtected": false,
      "SignalExpression": null,
      "Id": "dashboard-retry-overview",
      "Charts": [
        {
          "Id": "chart-1",
          "Title": "Total Retry Attempts",
          "SignalExpression": null,
          "Queries": [
            {
              "Id": "query-1",
              "Measurements": [
                {
                  "Value": "count(*)",
                  "Label": "Retries"
                }
              ],
              "Where": "@Message like '%retry%' ci and @Message like '%Attempt%' ci",
              "SignalExpression": null,
              "GroupBy": [],
              "DisplayStyle": {
                "Type": "Value",
                "LineFillToZeroY": false,
                "LineShowMarkers": true,
                "BarOverlaySum": false,
                "UseLogarithmicScale": false,
                "SuppressLegend": false,
                "Palette": "Default"
              },
              "Having": null,
              "OrderBy": [],
              "Limit": null
            }
          ],
          "DisplayStyle": {
            "WidthColumns": 3,
            "HeightRows": 1
          },
          "Description": "Total retry attempts in the last hour"
        },
        {
          "Id": "chart-2",
          "Title": "Retry Attempts by Service",
          "SignalExpression": null,
          "Queries": [
            {
              "Id": "query-2",
              "Measurements": [
                {
                  "Value": "count(*)",
                  "Label": "count"
                }
              ],
              "Where": "@Message like '%retry%' ci",
              "SignalExpression": null,
              "GroupBy": ["Service"],
              "DisplayStyle": {
                "Type": "Bar",
                "LineFillToZeroY": false,
                "LineShowMarkers": true,
                "BarOverlaySum": false,
                "UseLogarithmicScale": false,
                "SuppressLegend": false,
                "Palette": "Default"
              },
              "Having": null,
              "OrderBy": [{"Column": "count(*)", "Direction": "Desc"}],
              "Limit": null
            }
          ],
          "DisplayStyle": {
            "WidthColumns": 9,
            "HeightRows": 1
          },
          "Description": "Bar chart showing retry count per service"
        },
        {
          "Id": "chart-3",
          "Title": "Retry Rate Over Time",
          "SignalExpression": null,
          "Queries": [
            {
              "Id": "query-3",
              "Measurements": [
                {
                  "Value": "count(*)",
                  "Label": "Retries"
                }
              ],
              "Where": "@Message like '%retry%' ci",
              "SignalExpression": null,
              "GroupBy": [],
              "DisplayStyle": {
                "Type": "Line",
                "LineFillToZeroY": true,
