          "Title": "System Health Score",
          "SignalExpression": null,
          "Queries": [
            {
              "Id": "query-5",
              "Measurements": [
                {
                  "Value": "100 - (count(case when @Level in ['Error', 'Fatal'] then 1 end) * 100.0 / count(*))",
                  "Label": "Health %"
                }
              ],
              "Where": null,
              "SignalExpression": null,
              "GroupBy": [],
              "DisplayStyle": {
                "Type": "Value",
                "LineFillToZeroY": false,
