        {
          "Id": "chart-4",
          "Title": "Failed Operations (After All Retries)",
          "SignalExpression": null,
          "Queries": [
            {
              "Id": "query-4",
              "Measurements": [
                {
                  "Value": "count(*)",
                  "Label": "Failed"
                }
              ],
              "Where": "(@Message like '%failed after%retries%' ci or @Message like '%Moving to DLQ%' ci)",
              "SignalExpression": null,
              "GroupBy": [],
              "DisplayStyle": {
                "Type": "Bar",
                "LineFillToZeroY": false,
                "LineShowMarkers": true,
