{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Host=${USERDB_HOST};Port=${USERDB_PORT};Database=${USERDB_NAME};Username=${USERDB_USER};Password=${USERDB_PASSWORD}"
  },
  "Jwt": {
    "Key": "${JWT_SECRET_KEY}",
    "Issuer": "${JWT_ISSUER}",
    "Audience": "${JWT_AUDIENCE}",
    "ExpiryInMinutes": "${JWT_EXPIRY_MINUTES}"
  }
}
